<div class="container-login100" style="background-image: url('/assets/bng.png');">
      <div class="wrap-login100 p-l-55 p-r-55 p-t-65 p-b-54">
<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
          <span class="login100-form-title p-b-49">
            Registration Form
          </span>

           <div class="wrap-input100 validate-input m-b-23" >
            <span class="label-input100">First Name</span>
            <input class="input100" type="text" name="user[first_name]"  placeholder="Type your First Name">
            <span class="focus-input100" data-symbol="&#xf206;"></span>
          </div>

           <div class="wrap-input100 validate-input m-b-23" >
            <span class="label-input100">Last Name</span>
            <input class="input100" type="text" name="user[last_name]"  placeholder="Type your Last Name">
            <span class="focus-input100" data-symbol="&#xf206;"></span>
          </div>

           <div class="wrap-input100 validate-input m-b-23" >
            <span class="label-input100">Email</span>
            <input class="input100" type="text" name="user[email]"  placeholder="Type your Email">
            <span class="focus-input100" data-symbol="&#xf206;"></span>
          </div>

           <div class="wrap-input100 validate-input m-b-23" >
            <span class="label-input100">Password</span>
            <input class="input100" type="password" name="user[password]"  placeholder="Type your Password">
            <span class="focus-input100" data-symbol="&#xf206;"></span>
          </div>

          <div class="wrap-input100 validate-input m-b-23" >
            <span class="label-input100">Password Confirmation</span>
            <input class="input100" type="text" name="user[password_confirmation]"  placeholder="Re-Enter Password">
            <span class="focus-input100" data-symbol="&#xf206;"></span>
          </div>

          
          <div class="container-login100-form-btn">
            <div class="wrap-login100-form-btn">
              <div class="login100-form-bgbtn"></div>
              <button class="login100-form-btn">
                Sign Up
              </button>
            </div>
          </div>
          <br/>

          
                  <a href="<%= new_user_session_path %>"  class="txt2">
                Login
              </a>
             


   
          </div>
        </form>
        <% end %>
      </div>
    </div>


















  
